    <nestedClassifier xmi:type="uml:Class" xmi:id="_MV43kLMXEeuZeNFAWdsbSw" name="Main">
      <eAnnotations xmi:id="_MV43kbMXEeuZeNFAWdsbSw" source="http://www.eclipse.org/uml2/2.0.0/UML">
